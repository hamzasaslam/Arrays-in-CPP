#include <iostream>
using namespace std;
int main()
{
    int numbers[5];
    cout << "Enter 5 elements\n";
    for (int i = 0; i < 5; i++)
    {
        cin >> numbers[i];
    }
    cout << "Your array is\n";
    cout << "[";
    for (int a = 0; a < 5; a++)
    {
        cout << numbers[a];
    }
    cout << "]";
    return 0;
}
