// Multidimentional Array
#include <iostream>
using namespace std;
int main()
{
    int array[3][2] = {{1, 2},
                       {3, 4},
                       {5, 6}};
    // using nested for loop to print such array
    for (int i = 0; i < 3; ++i)
    {
        for (int j = 0; j < 2; ++j)
        {
            cout << "array[" << i << "]"
                 << "[" << j << "]=" << array[i][j] << endl;
        }
    }
    return 0;
}
